{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "text",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "published": {
      "type": "date",
      "required": true
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::blog-category.blog-category",
      "inversedBy": "blogs"
    },
    "related": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog.blog"
    },
    "body": {
      "type": "dynamiczone",
      "components": [
        "sections.hero-image",
        "sections.slide",
        "content.simple-body"
      ],
      "required": true,
      "max": 10,
      "min": 1
    },
    "cta": {
      "type": "dynamiczone",
      "components": [
        "call-to-action.download",
        "call-to-action.question"
      ],
      "required": false,
      "max": 2,
      "min": 0
    },
    "media": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo",
      "required": true
    },
    "medium": {
      "type": "string"
    },
    "learn": {
      "type": "boolean"
    },
    "readingtime": {
      "type": "integer",
      "required": true
    }
  }
}
